#include <iostream>
#include <cstdlib>
#include <ctime>

using namespace std;

int main() {
    int secretNumber;
    int guess;

    srand(time(0));
    secretNumber = (rand() % 100) + 1;

    while (true) {
        cin >> guess;

        if (guess < secretNumber) {
            cout << "Too low" << endl;
        } else if (guess > secretNumber) {
            cout << "Too high" << endl;
        } else {
            cout << "Correct" << endl;
            break;
        }
    }

    return 0;
}
